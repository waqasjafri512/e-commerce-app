<%- include('../includes/head.ejs') %>
</head>
<body>
<%- include('../includes/navigation.ejs') %>
<main>
  <h1>Your Wishlist</h1>
  <% if (!products.length) { %><p>No favorites yet.</p><% } %>
  <% products.forEach(product => { %>
    <p><a href="/products/<%= product._id %>"><%= product.title %></a> - $<%= product.price %></p>
  <% }) %>
</main>
<%- include('../includes/end.ejs') %>
